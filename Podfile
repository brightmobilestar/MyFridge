# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'MyFridge' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for MyFridge

	pod 'Firebase/Auth'	
	pod 'Firebase/Core'
	pod 'Firebase/Database'
	pod 'FirebaseStorage'
	
	pod 'IQKeyboardManagerSwift', :path=>"https://github.com/hackiftekhar/IQKeyboardManager.git"

	pod 'SDWebImage'

	pod 'PKHUD'
	pod 'SVProgressHUD'
	

  target 'MyFridgeTests' do
    inherit! :search_paths
    # Pods for testing

  end

  target 'MyFridgeUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
